//write a java progaram to implement interface using extends keyword
// basic interface
interface Printable{
    void print();
}
// derived interface extends base interface
interface Showable extends Printable {
    void show();
}

// class implements showalbe (and must implement both print (), show();

class Demo implements Showable{
    public void print(){
        System.out.println("print method called");
    }
    public void show(){
        System.out.println("Show method called");
    }
}
public class Main{
    public static void main (String[] args){
    Demo obj = new Demo();
    obj.print();
    obj.show();
    }
}

